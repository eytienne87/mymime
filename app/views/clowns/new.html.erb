<h1>This is the page to create a mime</h1>

<%= simple_form_for(@clown) do |f| %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :years_of_experience %>
    <%= f.input :gender %>
    <%= f.input :rental_duration %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
