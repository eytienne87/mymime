<div class="booking-form">
  <h1>List a New Mime</h1>
  <%= simple_form_for(@clown) do |f| %>
    <%= f.input :name %>
    <%= f.input :years_of_experience %>
    <%= f.input :description %>
    <%= f.input :price %>
    <%= f.input :photo, as: :file %>
    <div>
      <%= f.button :submit, "Add a mime", class: 'btn btn-danger btn-sm mt-2 mr-1' %>
    </div>
  <% end %>
</div>
