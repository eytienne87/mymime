<h1>List a New Mime</h1>

<div class="booking-form">
  <%= simple_form_for(@clown) do |f| %>
    <%= f.input :name %>
    <%= f.input :years_of_experience %>
    <%= f.input :description %>
    <%= f.input :price %>
    <%= f.input :photo, as: :file %>
  <% end %>
</div>

<div class="btn btn-danger">
  <%= f.button :submit %>
</div>
